format ELF64
public tmpl_LDouble_Floor

tmpl_LDouble_Floor:
    fnstcw  [rsp - 10]
    fld     tword [rsp + 8]
    movzx   eax, word [rsp - 10]
    and     ah, -13
    or      ah, 4
    mov     [rsp - 12], ax
    fldcw   [rsp - 12]
    frndint
    fldcw   [rsp - 10]
    ret

section '.note.GNU-stack'
