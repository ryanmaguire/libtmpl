format ELF64
public tmpl_Double_Fractional_Part

tmpl_Double_Fractional_Part:
    roundsd xmm1, xmm0, 0x3 ; xmm1 = trunc(xmm0)
    subsd xmm0, xmm1        ; xmm0 = xmm0 - xmm1
    ret                     ; return xmm0

section '.note.GNU-stack'
