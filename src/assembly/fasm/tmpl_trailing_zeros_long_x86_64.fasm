format ELF64
public tmpl_Long_Trailing_Zeros

tmpl_Long_Trailing_Zeros:
    xor       edx, edx
    xor       eax, eax
    rep bsf   rdx, rdi
    test      rdi, rdi
    cmovne    rax, rdx
    ret
