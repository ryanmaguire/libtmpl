include_guard()
set(headers @headers@)
foreach(header IN LISTS headers)
    string(FIND ${header} "${CMAKE_CURRENT_SOURCE_DIR}" index)
    if(index GREATER_EQUAL 0)
        cmake_path(RELATIVE_PATH header)
        list(APPEND relative_headers ${header})
    endif()
endforeach()

target_sources(
    tmpl
    PUBLIC
        FILE_SET HEADERS
        FILES
            ${relative_headers}
)
